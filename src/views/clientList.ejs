      <script src='/scripts/animated.js'></script>

</head>


<body class='clientList'>

  <!-- <h1>CLIENT DIRECTORY</h1> -->

  <div class="container" id='clientlist'>
    <div class="table-responsive">
      <div class="table-wrapper">
        <table class="table table-striped table-hover" id='ExportDataTable'>
          <thead style="background: #1D3557; text-align:center; color:#F1FAEE;">
            <tr>
              <th>NAME</th>
              <th>APPLICATION DATE</th>
              <th>STATUS</th>
              <TH>ASSIGNED TO</TH>
            </tr>
          </thead>
          <tbody>
            <% for (let i=0; i < result.rows.length; i++){ %>

              <tr>
                <td><a href='clientfile/<%= result.rows[i].id %>'>
                    <%= result.rows[i].name %>
                  </a>
                </td>
                <td>
                  <%= result.rows[i].applied_at.toString().substring(0,16) %>
                </td>
                <td>
                  <%= result.rows[i].status %>
                </td>
                <td>
                  <%= result.rows[i].user_id %>
                </td>
              </tr>
              <% } %>

          </tbody>
        </table>
      </div>
    </div>

</body>

</html>